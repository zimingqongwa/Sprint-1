//Program to print out student information
public class StudentDetails {
   public static void main(String[] args) {
      String name = "John";
      String surname = "Adams";
      String dateofbirth = "25 March 2001";
      String learnernumber = "074";
      String parentscontact = "0788104991/0785818183";
      String address = "Marikana,main store 997";
      
      System.out.println("full name: " + name);
      
      System.out.println("surname: " + surname);
      System.out.println("date of birth: " + dateofbirth );
      System.out.println("learner number: " + learnernumber);
      System.out.println("parents contact: " + parentscontact);
      System.out.println("address: " + address);
   }
}
